<Display>
  <Maps>
    <Map_1>
      <display>Bars</display>
      <bar_width>0.1</bar_width>
      <bar_min_high>0</bar_min_high>
      <bar_max_high>5</bar_max_high>
    </Map_1>
    <Map_2>
      <display>Circles</display>
      <count_levels>5</count_levels>
      <size_min>10</size_min>
      <size_max>50</size_max>
    </Map_2>
    <Map_3>
      <display>Cloropeth</display>
      <count_levels>5</count_levels>
    </Map_3>
    <Map_4>
      <display>MultiCloropeth</display>
      <count_levels>5</count_levels>
    </Map_4>
    <Map_5>
      <display>Sectors</display>
      <R>5</R>
      <resolution>10</resolution>
    </Map_5>
  </Maps>
  <Display_rule>
    <NB_max>100</NB_max>
    <Rule>
      <preference>3</preference>
      <name>Bars</name>
      <Conditions>
        <NB>
          <NB_min>2</NB_min>
          <NB_max>6</NB_max>
        </NB>
        <Dimensions>
          <Dimension>
            <name>Measures</name>
            <NB_min>*</NB_min>
            <NB_max>*</NB_max>
          </Dimension>
          <Dimension>
            <name>date</name>
            <NB_min>*</NB_min>
            <NB_max>*</NB_max>
          </Dimension>
          <Dimension>
            <name>species</name>
            <NB_min>*</NB_min>
            <NB_max>*</NB_max>
          </Dimension>
          <Dimension>
            <name>atlas</name>
            <NB_min>*</NB_min>
            <NB_max>*</NB_max>
          </Dimension>
        </Dimensions>
      </Conditions>
      <Actions>
        <Measure>
          <name>nbr of atlas</name>
          <Map>Map_1</Map>
          <color>#991055</color>
        </Measure>
        <Measure>
          <name>max counted atlas</name>
          <Map>Map_1</Map>
          <color>#109955</color>
        </Measure>
        <Measure>
          <name>atlas occurence/ max occured</name>
          <Map>Map_1</Map>
          <color>#551099</color>
        </Measure>
      </Actions>
    </Rule>
    <Rule>
      <preference>2</preference>
      <name>Multi cloropeth</name>
      <Conditions>
        <NB>
          <NB_min>7</NB_min>
          <NB_max>*</NB_max>
        </NB>
        <Dimensions>
          <Dimension>
            <name>Measures</name>
            <NB_min>*</NB_min>
            <NB_max>*</NB_max>
          </Dimension>
          <Dimension>
            <name>date</name>
            <NB_min>*</NB_min>
            <NB_max>*</NB_max>
          </Dimension>
          <Dimension>
            <name>species</name>
            <NB_min>*</NB_min>
            <NB_max>*</NB_max>
          </Dimension>
          <Dimension>
            <name>atlas</name>
            <NB_min>*</NB_min>
            <NB_max>*</NB_max>
          </Dimension>
        </Dimensions>
      </Conditions>
      <Actions>
        <Measure>
          <name>nbr of atlas</name>
          <Map>Map_4</Map>
          <color>#991055</color>
        </Measure>
        <Measure>
          <name>max counted atlas</name>
          <Map>Map_4</Map>
          <color>#109955</color>
        </Measure>
        <Measure>
          <name>atlas occurence/ max occured</name>
          <Map>Map_4</Map>
          <color>#551099</color>
        </Measure>
      </Actions>
    </Rule>
    <Rule>
      <preference>1</preference>
      <name>Affichage Choropleth unique</name>
      <Conditions>
        <NB>
          <NB_min>1</NB_min>
          <NB_max>1</NB_max>
        </NB>
        <Dimensions>
          <Dimension>
            <name>Measures</name>
            <NB_min>*</NB_min>
            <NB_max>*</NB_max>
          </Dimension>
          <Dimension>
            <name>date</name>
            <NB_min>*</NB_min>
            <NB_max>*</NB_max>
          </Dimension>
          <Dimension>
            <name>species</name>
            <NB_min>*</NB_min>
            <NB_max>*</NB_max>
          </Dimension>
          <Dimension>
            <name>atlas</name>
            <NB_min>*</NB_min>
            <NB_max>*</NB_max>
          </Dimension>
        </Dimensions>
      </Conditions>
      <Actions>
        <Measure>
          <name>nbr of atlas</name>
          <Map>Map_3</Map>
          <color>#991055</color>
        </Measure>
        <Measure>
          <name>max counted atlas</name>
          <Map>Map_3</Map>
          <color>#109955</color>
        </Measure>
        <Measure>
          <name>atlas occurence/ max occured</name>
          <Map>Map_3</Map>
          <color>#551099</color>
        </Measure>
      </Actions>
    </Rule>
    <Rule>
      <preference>1</preference>
      <name>Affichage Choropleth Circle</name>
      <Conditions>
        <NB>
          <NB_min>2</NB_min>
          <NB_max>2</NB_max>
        </NB>
        <Dimensions>
          <Dimension>
            <name>Measures</name>
            <NB_min>2</NB_min>
            <NB_max>*</NB_max>
          </Dimension>
          <Dimension>
            <name>date</name>
            <NB_min>*</NB_min>
            <NB_max>*</NB_max>
          </Dimension>
          <Dimension>
            <name>species</name>
            <NB_min>*</NB_min>
            <NB_max>*</NB_max>
          </Dimension>
        </Dimensions>
        <Dimension>
          <name>atlas</name>
          <NB_min>*</NB_min>
          <NB_max>*</NB_max>
        </Dimension>
      </Conditions>
      <Actions>
        <Measure>
          <name>nbr of atlas</name>
          <Map>Map_3</Map>
          <color>#991055</color>
        </Measure>
        <Measure>
          <name>max counted atlas</name>
          <Map>Map_1</Map>
          <color>#109955</color>
        </Measure>
        <Measure>
          <name>atlas occurence/ max occured</name>
          <Map>Map_2</Map>
          <color>#551099</color>
        </Measure>
      </Actions>
    </Rule>
  </Display_rule>
  <MAP_Active>
    <Measure>
      <name>nbr of atlas</name>
      <display>MultiCloropeth</display>
      <count_levels>5</count_levels>
      <color>#991055</color>
    </Measure>
    <Measure>
      <name>max counted atlas</name>
      <display>MultiCloropeth</display>
      <count_levels>5</count_levels>
      <color>#109955</color>
    </Measure>
    <Measure>
      <name>atlas occurence/ max occured</name>
      <display>MultiCloropeth</display>
      <count_levels>5</count_levels>
      <color>#551099</color>
    </Measure>
  </MAP_Active>
  <Background>
    <color>#	</color>
  </Background>
</Display>