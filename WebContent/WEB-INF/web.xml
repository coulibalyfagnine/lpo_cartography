<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>LPO_geoCartography</display-name>
  <servlet>
    <servlet-name>TraitementDonnees</servlet-name>
    <servlet-class>Servlet.TraitementDonnees</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TraitementDonnees</servlet-name>
    <url-pattern>/TraitementDonnees</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>Accueil</servlet-name>
    <servlet-class>Servlet.Accueil</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Accueil</servlet-name>
    <url-pattern>/Acceuil</url-pattern>
  </servlet-mapping>
  
    <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <welcome-file-list>

    <welcome-file>/jsp/accueil.jsp</welcome-file>
 
  </welcome-file-list>
</web-app>